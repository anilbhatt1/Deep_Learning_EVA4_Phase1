      "cell_type": "code",
      "metadata": {
        "id": "mLGOYEershIC",
        "colab_type": "code",
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 711
        },
        "outputId": "dcd7cb13-a9fa-47ec-b546-0f51789cea96"
      },
      "source": [
        "a = torch.zeros(3, 2)\n",
        "a_transpose = torch.transpose(a,1,0)\n",
        "print('a:',a)\n",
        "print('a.size:', a.size())\n",
        "print('a_transpose',a_transpose)\n",
        "print('a_transpose.size:', a_transpose.size())\n",
        "print('\\n')\n",
        "b = torch.ones(3, 2)\n",
        "b_transpose = torch.transpose(b,1,0)\n",
        "print('b:',b)\n",
        "print('b.size:', b.size())\n",
        "print('b_transpose',b_transpose)\n",
        "print('b_transpose.size:', b_transpose.size())\n",
        "print('\\n')\n",
        "c = torch.cat((a,b),dim=0)\n",
        "print('c:',c)\n",
        "print('c.size:', c.size())\n",
        "print('\\n')\n",
        "d = torch.transpose(c,1,0)\n",
        "print('d:',c)\n",
        "print('d.size:', d.size())\n",
        "print('\\n')\n",
        "e = torch.cat((a,b),dim=1)\n",
        "print('e:',e)\n",
        "print('e.size:', e.size())"
      ],
      "execution_count": 53,
      "outputs": [
