      },
      "source": [
        "a = torch.arange(start=0, end=10)\n",
        "indices = np.arange(0, 10) % 6\n",
        "print(indices)\n",
        "print(indices[-5:])\n",
        "b = a[indices[-5:]]\n",
        "print(b)\n",
        "\n",
        "a = torch.arange(start=0, end=10)\n",
        "indices = np.arange(1, 11) % 6\n",
        "print(indices)\n",
        "print(indices[-5:])\n",
        "b = a[indices[-5:]]\n",
        "print(b)"
